# Waypoint Ingestion Pipeline Configuration
# No API key required - uses ChromaDB's built-in default embeddings

# Embedding config (ChromaDB default: all-MiniLM-L6-v2 via ONNX)
EMBEDDING_MODEL=default
EMBEDDING_DIMENSIONS=384

# ChromaDB storage directory (relative to 02_ingestion_pipeline/)
CHROMA_PERSIST_PATH=./chroma_db

# ChromaDB collection name
COLLECTION_NAME=waypoint_kb

# Path to knowledge base documents (relative to 02_ingestion_pipeline/)
KNOWLEDGE_BASE_PATH=../01_knowledge_base

# Logging verbosity (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
